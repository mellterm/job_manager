<h1>Listing jobs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Domain</th>
    <th>Notes</th>
    <th>Deadline</th>
    <th>Directory</th>
    <th>Company</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobs.each do |job| %>
  <tr>
    <td><%= job.name %></td>
    <td><%= job.domain_id %></td>
    <td><%= job.notes %></td>
    <td><%= job.deadline %></td>
    <td><%= job.directory %></td>
    <td><%= job.company_id %></td>
    <td><%= link_to 'Show', job %></td>
    <td><%= link_to 'Edit', edit_job_path(job) %></td>
    <td><%= link_to 'Destroy', job, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Job', new_job_path %>
